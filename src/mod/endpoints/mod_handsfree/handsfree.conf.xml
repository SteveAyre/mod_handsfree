<configuration name="handsfree.conf" description="Handsfree Endpoint">

  <settings>
    <!--Default dialplan -->
    <param name="dialplan" value="XML"/>
    <param name="context" value="default"/>
  </settings>

  <!-- which modems to register -->
  <modems>
  	<!-- name is the bluetooth device name as paired -->
  	<modem name="Moycell">
  		<!-- where to send the incoming calls to. For outbound you can dial bridge(handsfree/Moycell/<number-to-dial>) -->
    		<param name="dialplan" value="XML"/>
    		<param name="context" value="default"/>
	</modem>
  </modems>

</configuration>

